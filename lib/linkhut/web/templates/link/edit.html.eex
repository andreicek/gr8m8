<h3>Edit link</h3>
<%= form_for @changeset, Routes.link_path(@conn, :update), fn f -> %>
  <fieldset>
  <%= hidden_input f, :url %>
  <%= input f, :url, type: :url_input, disabled: true %>
  <%= input f, :title %>
  <%= input f, :notes, type: :textarea, maxlength: 1024 %>
  <%= input f, :tags %>
  <%= input f, :is_private, type: :checkbox %>
  </fieldset>
  <%= submit "Submit" %>
<% end %>
